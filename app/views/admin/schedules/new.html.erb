<%= form_with model: [:admin, @movie, @schedule], local: true do |f| %>
  <%= f.label :schedule_date %>
  <%= f.date_select :schedule_date %>

  <%= f.label :start_time %>
  <%= f.time_select :start_time %>

  <%= f.label :end_time %>
  <%= f.time_select :end_time %>

  <%= f.label :screen_id %>
  <%= f.collection_select :screen_id, Screen.all, :id, :screen_number %>

  <%= f.submit 'スケジュール作成' %>
<% end %>
